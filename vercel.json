{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "DATABASE_URL": "your-database-url",
    "API_KEY": "your-api-key"
  },
  "cron": [
    {
      "path": "/api/clean-up",
      "schedule": "0 0 * * *"
    }
  ],
  "regions": ["syd1"], 
  "buildCommand": "npm run build",
  "outputDirectory": "build"
}
